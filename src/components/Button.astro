---
interface Props {
  title: string;
  redirect?: string;
  type?: "button" | "submit" | "reset" | null | undefined;
  styles?: string;
  onClick?: string;
}
import { getSession } from "auth-astro/server";
const session = await getSession(Astro.request);

const { title, redirect, styles, type, onClick } = Astro.props;
---

<button
  class:list={["rounded-md py-2 font-medium w-full", styles]}
  type={type || "button"}
  onclick={onClick}
  ;
>
  <a href={redirect}>{title}</a>
</button>
